{
  "name": "myfarmstand-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:hooks": "jest --config jest.config.hooks.regular.js src/hooks/__tests__/ --forceExit",
    "test:hooks:watch": "jest --config jest.config.hooks.regular.js --watch src/hooks/__tests__/",
    "test:hooks:race": "jest --config jest.config.hooks.race.js src/hooks/__tests__/ --forceExit",
    "test:hooks:race:watch": "jest --config jest.config.hooks.race.js --watch src/hooks/__tests__/",
    "test:race:cart": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useCart.race.test.tsx --forceExit",
    "test:race:orders": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useOrders.race.test.tsx --forceExit",
    "test:race:realtime": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useCentralizedRealtime.race.test.tsx --forceExit",
    "test:race:auth": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useAuth.race.test.tsx --forceExit",
    "test:race:stock": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useStockValidation.race.test.tsx --forceExit",
    "test:race:notifications": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useNotifications.race.test.tsx --forceExit",
    "test:race:products": "jest --config jest.config.hooks.race.js src/hooks/__tests__/useProducts.race.test.tsx --forceExit",
    "test:race:pickup": "jest --config jest.config.hooks.race.js src/hooks/__tests__/usePickupRescheduling.race.test.tsx --forceExit",
    "test:race:core": "npm run test:race:cart && npm run test:race:orders && npm run test:race:realtime && npm run test:race:auth && npm run test:race:stock && npm run test:race:notifications && npm run test:race:products && npm run test:race:pickup",
    "test:services": "TEST_TYPE=services jest --config jest.config.services.js src/services/",
    "test:services:watch": "TEST_TYPE=services jest --config jest.config.services.js --watch src/services/",
    "test:all": "npm run test:services && npm run test:hooks",
    "test:checkout": "jest CheckoutScreen",
    "test:confirmation": "jest OrderConfirmationScreen",
    "audit-and-fix": "ts-node automation/workflow-runner.ts",
    "audit-services": "ts-node automation/audit-services.ts",
    "validate-schemas": "ts-node automation/schema-validator.ts",
    "generate-fixes": "ts-node automation/pattern-fixer.ts",
    "apply-fixes": "ts-node automation/pattern-fixer.ts --apply",
    "generate-tests": "ts-node automation/test-generator.ts",
    "quick-audit": "ts-node automation/workflow-runner.ts quick",
    "full-automation": "ts-node automation/workflow-runner.ts full",
    "audit-only": "ts-node automation/workflow-runner.ts audit",
    "fix-only": "ts-node automation/workflow-runner.ts fix",
    "sync-schema": "node scripts/sync-supabase-schema.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.4.3",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/drawer": "^7.5.5",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/stack": "^7.4.4",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "crypto-js": "^4.2.0",
    "expo": "~53.0.20",
    "expo-barcode-scanner": "~13.0.1",
    "expo-camera": "~16.1.11",
    "expo-font": "~13.3.2",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "~19.0.10",
    "@types/uuid": "^10.0.0",
    "jest-expo": "^53.0.9",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
