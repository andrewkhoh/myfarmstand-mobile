{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_ENV": "development",
        "EXPO_PUBLIC_ALLOW_DEV_SCRIPTS": "true"
      }
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "NODE_ENV": "staging",
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_ALLOW_DEV_SCRIPTS": "false"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_ALLOW_DEV_SCRIPTS": "false",
        "EXPO_PUBLIC_SHOW_TESTS": "false"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "aab"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_ENV": "preview",
        "EXPO_PUBLIC_ALLOW_DEV_SCRIPTS": "false"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "XXXXXXXXXX"
      },
      "android": {
        "serviceAccountKeyPath": "./secrets/google-service-account.json",
        "track": "internal"
      }
    }
  }
}